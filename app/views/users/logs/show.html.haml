- content_for(:title) { "Logs: #{@user.name}" }

- user_p = present(@user)

%h1 Logs for #{user_p.link}

%hr

.panel.panel-info
  .panel-heading Tools
  .panel-body
    = link_to alts_user_logs_path(@user), class: 'btn btn-default' do
      Alternate Accounts
      .glyphicon.glyphicon-user

%table.table.table-striped
  %thead
    %tr
      %th IP Address
      %th First Seen
      %th Browser
      %th Operating System
  %tbody
    - @visits.each do |visit|
      - visit_p = present(visit)
      %tr
        %td= visit.ip
        %td= visit_p.started_at
        %td= visit.browser
        %td= visit.os

